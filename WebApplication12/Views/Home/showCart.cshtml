@model IEnumerable<WebApplication12.EF.Cart>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.cart_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Product_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.category_id)
        </th>
        <th></th>
    </tr>

    

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.cart_id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Product_id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.category_id)
                </td>
                <td>
                    @Html.ActionLink("Place Order", "placeOrder", new { id = item.cart_id }) |
                    @Html.ActionLink("Back To Product List", "showAllDetails") |
                </td>
            </tr>
        }
    

</table>
